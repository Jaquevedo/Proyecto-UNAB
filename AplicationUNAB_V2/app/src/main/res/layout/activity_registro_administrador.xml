<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/u_abc_bcgn"
    android:backgroundTint="#C5D8DC"
    tools:context=".activitys.registro.RegistroAdministrador">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="320dp"
        android:layout_marginBottom="16dp"
        android:contentDescription="@string/todo4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/u_abc" />

    <ImageView
        android:id="@+id/ImageV2"
        android:layout_width="140dp"
        android:layout_height="144dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="22dp"
        app:layout_constraintBottom_toTopOf="@+id/Butt9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewEstudiante"
        app:srcCompat="@drawable/camera"
        tools:ignore="ContentDescription,ImageContrastCheck" />

    <Button
        android:id="@+id/Butt9"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/buttons_styles2"
        android:backgroundTint="#018685"
        android:fontFamily="@font/comfortaa_light"
        android:text="@string/Butt9"
        android:textAllCaps="false"
        android:textColor="@color/white"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ImageV2"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/TextV9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/comfortaa_light"
        android:text="@string/TextV9"
        android:textAlignment="center"
        android:textColor="#025756"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Butt9" />

    <TableLayout
        android:id="@+id/TablLayout1"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TextV9"
        tools:ignore="MissingConstraints">

        <TableRow android:background="@color/clarito">

            <TextView
                android:id="@+id/tCorreo"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:background="@drawable/border_tablelayout"
                android:backgroundTint="#99E0E0"
                android:fontFamily="@font/comfortaa_light"
                android:padding="10sp"
                android:text="@string/txtCorreo"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="TextContrastCheck" />

            <EditText
                android:id="@+id/txtCorreo"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:autofillHints=""
                android:background="@drawable/border_tablelayout1"
                android:backgroundTint="#99E0E0"
                android:fontFamily="@font/comfortaa_light"
                android:inputType="textEmailAddress"
                android:padding="10sp"
                android:text=""
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
        </TableRow>

        <TableRow android:background="@color/azul_claro">

            <TextView
                android:id="@+id/TabLyTextV1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:background="@drawable/border_tablelayout"
                android:backgroundTint="#018685"
                android:fontFamily="@font/comfortaa_light"
                android:padding="10sp"
                android:text="@string/row1"
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="TextContrastCheck" />

            <EditText
                android:id="@+id/txtNombre"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:autofillHints=""
                android:background="@drawable/border_tablelayout"
                android:backgroundTint="#018685"
                android:fontFamily="@font/comfortaa_bold"
                android:inputType="text"
                android:padding="10sp"
                android:text=""
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="TextContrastCheck,TouchTargetSizeCheck,SpeakableTextPresentCheck" />

        </TableRow>

        <TableRow android:background="@color/clarito">

            <TextView
                android:id="@+id/TabLyTextV3"
                android:layout_width="59dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:background="@drawable/border_tablelayout1"
                android:backgroundTint="#99E0E0"
                android:fontFamily="@font/comfortaa_light"
                android:padding="10sp"
                android:text="@string/doc"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="TextContrastCheck" />

            <EditText
                android:id="@+id/txtApellido"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:autofillHints=""
                android:background="@drawable/border_tablelayout1"
                android:backgroundTint="#99E0E0"
                android:fontFamily="@font/comfortaa_bold"
                android:inputType="number"
                android:padding="10sp"
                android:text=""
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
        </TableRow>

        <TableRow android:background="@color/clarito">

            <TextView
                android:id="@+id/TabLyTextV7"
                android:layout_width="184dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:background="@drawable/border_tablelayout1"
                android:backgroundTint="#018685"
                android:fontFamily="@font/comfortaa_light"
                android:padding="10sp"
                android:text="@string/row4"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="TextContrastCheck" />

            <EditText
                android:id="@+id/txtPass"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:autofillHints=""
                android:background="@drawable/border_tablelayout1"
                android:backgroundTint="#018685"
                android:fontFamily="@font/comfortaa_bold"
                android:inputType="text|textPassword"
                android:padding="10sp"
                android:text=""
                android:textAlignment="viewStart"
                android:textColor="@color/black"
                android:textSize="16sp"
                tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />
        </TableRow>

    </TableLayout>

    <Button
        android:id="@+id/Butt10"
        android:layout_width="170dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/buttons_styles2"
        android:backgroundTint="#018685"
        android:fontFamily="@font/comfortaa_light"
        android:onClick="viewOption"
        android:text="@string/Butt10"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TablLayout1" />

    <ImageButton
        android:id="@+id/ImageButt11"
        android:layout_width="82dp"
        android:layout_height="80dp"
        android:layout_marginEnd="5dp"
        android:layout_marginBottom="8dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/salud_appx48"
        tools:ignore="ContentDescription,ImageContrastCheck,SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:clickable="true"
        android:onClick="viewOption"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back"
        tools:ignore="TouchTargetSizeCheck,SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="20dp"
        android:onClick="viewOption"
        android:tint="#FF0000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logout"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textViewEstudiante"
        android:layout_width="220dp"
        android:layout_height="62dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/comfortaa_bold"
        android:gravity="center"
        android:text="@string/Administrador"
        android:textColor="#018685"
        android:textSize="28dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/ImageV2"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>